{
  "name": "mattmilesi/pt-osc-command-generator",
  "description": "Generates pt-online-schema-change commands for MySQL, given the ALTER TABLE query",
  "version": "0.1.0",
  "license": "Apache-2.0",
  "type": "library",
  "require": {
    "php": ">=7.4",
    "phpmyadmin/sql-parser": "^5.9"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.48",
    "fattureincloud/php-code-standard": "^1.1",
    "phpunit/phpunit": "^9.6"
  },
  "autoload": {
    "psr-4": {
      "PtOscCommandGenerator\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/"
    ]
  }
}
